    def eredmeny_hirdetes(self):
        self.stop_timer()
        self.button_randomnumber.setText("Win!")
        msg = QMessageBox()
        msg.setWindowTitle("Gyozelem!")
        msg.setText("Gratulalunk! Nyertel!")
        msg.exec()
        self.file = open("player_records.txt", "a")
        print(self.textbox_input_nev.toPlainText() + " jatekosnak " + str(
            self.huzasok_szama) + " húzásra volt szüksége, hogy nyerjen", file=self.file)
        self.file.close()


    def button_1_1(self):
        #Ha talalat van
        if self.button_card_1_1.text() == self.button_randomnumber.text():
            self.button_card_1_1.setStyleSheet("background-color : black; color : white")
            self.button_card_1_1.setDisabled(True)

        #Ha vizszintesen kijott
        if self.button_card_1_1.isEnabled() == False and self.button_card_1_2.isEnabled() == False and self.button_card_1_3.isEnabled() == False and  self.button_card_1_4.isEnabled() == False and  self.button_card_1_5.isEnabled() == False:
            self.eredmeny_hirdetes()

        #Ha fuggolegesen kijott
