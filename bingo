    def retranslateUi(self, MainWindow):
        _translate = QtCore.QCoreApplication.translate
        MainWindow.setWindowTitle(_translate("MainWindow", "MainWindow"))
        self.button_startgame.setText(_translate("MainWindow", "Start game!"))
        self.label_name.setText(_translate("MainWindow", "Name:"))

    def generate_random_numbers(self):
        self.button_randomnumber.setText(str(random.randint(1,75)))
        self.huzasok_szama+=1

    def start_timer(self):
        self.timer.start(5000)
        self.huzasok_szama = 0

    def stop_timer(self):
        self.timer.stop()

    def start_game(self):
        if self.textbox_input_nev.toPlainText() == "":
            msg = QMessageBox()
            msg.setWindowTitle("Hiba!")
            msg.setText("Kotelezo nevet megadni!")
            msg.exec()
        else:
            self.button_startgame.setVisible(False)
            self.button_restartgame.setVisible(True)
            self.button_card_1_1.setVisible(True)
            self.button_card_1_2.setVisible(True)
            self.button_card_1_3.setVisible(True)
            self.button_card_1_4.setVisible(True)
            self.button_card_1_5.setVisible(True)
            self.button_card_2_1.setVisible(True)
            self.button_card_2_2.setVisible(True)
            self.button_card_2_3.setVisible(True)
            self.button_card_2_4.setVisible(True)
            self.button_card_2_5.setVisible(True)
            self.button_card_3_1.setVisible(True)
            self.button_card_3_2.setVisible(True)
            self.button_card_3_3.setVisible(True)
            self.button_card_3_4.setVisible(True)
            self.button_card_3_5.setVisible(True)
            self.button_card_4_1.setVisible(True)
            self.button_card_4_2.setVisible(True)
            self.button_card_4_3.setVisible(True)
            self.button_card_4_4.setVisible(True)
            self.button_card_4_5.setVisible(True)
            self.button_card_5_1.setVisible(True)
            self.button_card_5_2.setVisible(True)
            self.button_card_5_3.setVisible(True)
            self.button_card_5_4.setVisible(True)
            self.button_card_5_5.setVisible(True)
            self.button_randomnumber.setVisible(True)
            self.textbox_input_nev.setDisabled(True)
            self.textbox_input_nev.setStyleSheet("color:blue")


            for row in range(5):
                list_of_numbers = []
                for column in range(5):
                    list_of_numbers.append(random.randint(1, 75))
                list_of_numbers.sort()
                if row == 1:
                    self.button_card_1_1.setText(str(list_of_numbers[0]))
                    self.button_card_1_2.setText(str(list_of_numbers[1]))
                    self.button_card_1_3.setText(str(list_of_numbers[2]))
                    self.button_card_1_4.setText(str(list_of_numbers[3]))
                    self.button_card_1_5.setText(str(list_of_numbers[4]))
                elif row == 2:
                    self.button_card_2_1.setText(str(list_of_numbers[0]))
                    self.button_card_2_2.setText(str(list_of_numbers[1]))
                    self.button_card_2_3.setText(str(list_of_numbers[2]))
                    self.button_card_2_4.setText(str(list_of_numbers[3]))
                    self.button_card_2_5.setText(str(list_of_numbers[4]))
                elif row == 3:
                    self.button_card_3_1.setText(str(list_of_numbers[0]))
                    self.button_card_3_2.setText(str(list_of_numbers[1]))
                    self.button_card_3_4.setText(str(list_of_numbers[3]))
                    self.button_card_3_5.setText(str(list_of_numbers[4]))
                elif row == 4:
                    self.button_card_4_1.setText(str(list_of_numbers[0]))
                    self.button_card_4_2.setText(str(list_of_numbers[1]))
                    self.button_card_4_3.setText(str(list_of_numbers[2]))
                    self.button_card_4_4.setText(str(list_of_numbers[3]))
                    self.button_card_4_5.setText(str(list_of_numbers[4]))
                else:
                    self.button_card_5_1.setText(str(list_of_numbers[0]))
                    self.button_card_5_2.setText(str(list_of_numbers[1]))
                    self.button_card_5_3.setText(str(list_of_numbers[2]))
                    self.button_card_5_4.setText(str(list_of_numbers[3]))
                    self.button_card_5_5.setText(str(list_of_numbers[4]))
                self.start_timer()


    def restart_game(self):

